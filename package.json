{
   "name": "load-web-page-jsdom-mocha",
   "description": "Minimal example of loading a web page into jsdom and testing with mocha",
   "version": "1.0.0",
   "license": "MIT",
   "main": "spec.js",
   "repository": "github:dnajs/load-web-page-jsdom-mocha",
   "homepage": "https://github.com/dnajs/load-web-page-jsdom-mocha",
   "bugs": "https://github.com/dnajs/load-web-page-jsdom-mocha/issues",
   "docs": "https://github.com/dnajs/load-web-page-jsdom-mocha#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "jsdom",
      "nodejs",
      "localhost",
      "url",
      "testing",
      "javascript",
      "mocha"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "mocha": true,
      "browser": true,
      "jquery": true
   },
   "scripts": {
      "pretest": "jshint *.js",
      "test": "mocha spec*.js"
   },
   "devDependencies": {
      "canvas": "~2.6",
      "jsdom": "~15.2",
      "jshint": "~2.10",
      "mocha": "~7.0",
      "server-listening": "~0.1"
   },
   "dependencies": {}
}
