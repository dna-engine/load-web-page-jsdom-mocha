{
   "name": "load-web-page-jsdom-mocha",
   "description": "Minimal example of loading a web page into jsdom and testing with mocha",
   "version": "1.0.0",
   "license": "MIT",
   "type": "module",
   "module": "spec.js",
   "repository": "github:dna-engine/load-web-page-jsdom-mocha",
   "homepage": "https://github.com/dna-engine/load-web-page-jsdom-mocha",
   "bugs": "https://github.com/dna-engine/load-web-page-jsdom-mocha/issues",
   "docs": "https://github.com/dna-engine/load-web-page-jsdom-mocha#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "javascript",
      "jsdom",
      "localhost",
      "mocha",
      "nodejs",
      "testing",
      "url"
   ],
   "jshintConfig": {
      "esversion": 11,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "mocha": true,
      "browser": true
   },
   "scripts": {
      "pretest": "jshint . --exclude-path .gitignore",
      "test": "mocha spec*.js --timeout 5000"
   },
   "devDependencies": {
      "assert-deep-strict-equal": "~1.1",
      "jsdom": "~22.1",
      "jshint": "~2.13",
      "mocha": "~10.2",
      "server-listening": "~1.0"
   }
}
